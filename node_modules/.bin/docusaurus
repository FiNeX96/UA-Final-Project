#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/d479/Uni/3ANO/2SEMESTRE/PI/microsite/website/node_modules/.pnpm/@docusaurus+core@3.1.1_@docusaurus+types@3.1.1_react-dom@18.2.0_react@18.2.0_typescript@5.3.3/node_modules/@docusaurus/core/bin/node_modules:/home/d479/Uni/3ANO/2SEMESTRE/PI/microsite/website/node_modules/.pnpm/@docusaurus+core@3.1.1_@docusaurus+types@3.1.1_react-dom@18.2.0_react@18.2.0_typescript@5.3.3/node_modules/@docusaurus/core/node_modules:/home/d479/Uni/3ANO/2SEMESTRE/PI/microsite/website/node_modules/.pnpm/@docusaurus+core@3.1.1_@docusaurus+types@3.1.1_react-dom@18.2.0_react@18.2.0_typescript@5.3.3/node_modules/@docusaurus/node_modules:/home/d479/Uni/3ANO/2SEMESTRE/PI/microsite/website/node_modules/.pnpm/@docusaurus+core@3.1.1_@docusaurus+types@3.1.1_react-dom@18.2.0_react@18.2.0_typescript@5.3.3/node_modules:/home/d479/Uni/3ANO/2SEMESTRE/PI/microsite/website/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/d479/Uni/3ANO/2SEMESTRE/PI/microsite/website/node_modules/.pnpm/@docusaurus+core@3.1.1_@docusaurus+types@3.1.1_react-dom@18.2.0_react@18.2.0_typescript@5.3.3/node_modules/@docusaurus/core/bin/node_modules:/home/d479/Uni/3ANO/2SEMESTRE/PI/microsite/website/node_modules/.pnpm/@docusaurus+core@3.1.1_@docusaurus+types@3.1.1_react-dom@18.2.0_react@18.2.0_typescript@5.3.3/node_modules/@docusaurus/core/node_modules:/home/d479/Uni/3ANO/2SEMESTRE/PI/microsite/website/node_modules/.pnpm/@docusaurus+core@3.1.1_@docusaurus+types@3.1.1_react-dom@18.2.0_react@18.2.0_typescript@5.3.3/node_modules/@docusaurus/node_modules:/home/d479/Uni/3ANO/2SEMESTRE/PI/microsite/website/node_modules/.pnpm/@docusaurus+core@3.1.1_@docusaurus+types@3.1.1_react-dom@18.2.0_react@18.2.0_typescript@5.3.3/node_modules:/home/d479/Uni/3ANO/2SEMESTRE/PI/microsite/website/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@docusaurus/core/bin/docusaurus.mjs" "$@"
else
  exec node  "$basedir/../@docusaurus/core/bin/docusaurus.mjs" "$@"
fi
